# Frontend Environment Variables
# Copy this file to .env.local for local development
# Copy to .env.production for production build

# Backend API URL
# For local development (when running backend locally):
REACT_APP_API_URL=http://localhost:8000

# For production (replace with your deployed backend URL):
# REACT_APP_API_URL=https://your-backend.railway.app
# REACT_APP_API_URL=https://your-backend.render.com
# REACT_APP_API_URL=https://your-backend.fly.io

# OAuth Client IDs (public, safe to expose)
REACT_APP_GITHUB_CLIENT_ID=your-github-client-id
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Application
REACT_APP_VERSION=1.0.0
